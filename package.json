{
  "name": "ngx-umami-workspace",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "ng build ngx-umami",
    "build:watch": "ng build ngx-umami --watch",
    "test": "ng test ngx-umami",
    "test:coverage": "ng test ngx-umami --no-watch --code-coverage --browsers=ChromeHeadless",
    "lint": "ng lint ngx-umami",
    "lint:fix": "ng lint ngx-umami --fix",
    "format": "prettier --write \"projects/**/*.ts\"",
    "format:check": "prettier --check \"projects/**/*.ts\"",
    "version:patch": "cd projects/ngx-umami && npm version patch --no-git-tag-version",
    "version:minor": "cd projects/ngx-umami && npm version minor --no-git-tag-version",
    "version:major": "cd projects/ngx-umami && npm version major --no-git-tag-version",
    "prepublish:lib": "pnpm run lint && pnpm run test:coverage && pnpm run build",
    "publish:lib": "cd dist/ngx-umami && pnpm publish --access public"
  },
  "dependencies": {
    "@angular/common": "19.2.18",
    "@angular/core": "19.2.18",
    "@angular/router": "19.2.18",
    "rxjs": "7.8.0",
    "tslib": "2.6.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.18",
    "@angular-eslint/builder": "21.0.1",
    "@angular/cli": "19.2.18",
    "@angular/compiler": "19.2.18",
    "@angular/compiler-cli": "19.2.18",
    "@angular/platform-browser": "19.2.18",
    "@angular/platform-browser-dynamic": "19.2.18",
    "@eslint/js": "9.39.1",
    "@types/jasmine": "5.1.15",
    "angular-eslint": "21.0.1",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
    "jasmine-core": "5.1.2",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "ng-packagr": "19.2.2",
    "prettier": "3.8.0",
    "typescript": "5.6.3",
    "typescript-eslint": "8.46.4",
    "zone.js": "0.15.1"
  }
}
